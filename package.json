{
  "name": "d3-inertia",
  "version": "0.0.1",
  "description": "Inertia Dragging",
  "keywords": [
    "d3",
    "d3-module",
    "d3-drag",
    "d3-geo",
    "inertia"
  ],
  "license": "MIT",
  "main": "build/d3-inertia.js",
  "unpkg": "build/d3-inertia.js",
  "jsdelivr": "build/d3-inertia.js",
  "module": "index",
  "jsnext:main": "index",
  "homepage": "https://github.com/Fil/d3-inertia",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fil/d3-inertia.git"
  },
  "author": {
    "name": "Philippe Riviere",
    "url": "https://github.com/Fil"
  },
  "contributors": [
    {
      "name": "Mike Bostock",
      "url": "https://bost.ocks.org/mike/"
    },
    {
      "name": "Widi Harsojo",
      "url": "https://github.com/earthjs/earthjs"
    }
  ],
  "scripts": {
    "pretest": "rm -rf build && mkdir build && rollup --banner \"$(preamble)\" -g d3-drag:d3,d3-selection:d3,d3-timer:d3 -f umd -n d3 --extend d3 -o build/d3-inertia.js -- index.js",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "npm run test && uglifyjs --preamble \"$(preamble)\" build/d3-inertia.js -c negate_iife=false -m -o build/d3-inertia.min.js",
    "postpublish": "zip -j build/d3-inertia.zip -- LICENSE.txt README.md build/d3-inertia.js build/d3-inertia.min.js"
  },
  "dependencies": {
    "d3": "^4.0",
    "d3-drag": "^1.2.1",
    "d3-selection": "^1.1.0",
    "d3-timer": "^1.0.7",
    "versor": "^0.0.1"
  },
  "devDependencies": {
    "package-preamble": "0.1",
    "rollup": "0.49",
    "tape": "4",
    "uglify-js": "*"
  }
}
